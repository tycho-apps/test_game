			image_h = "/game/ui/main_menu/main_menu_mp_s.png";
		},
		widget button quit
		{
			trans = [ 0.5, 0.40, 0  ];
			scale = [ 0.4, 0.2, 1 ];
			image = "/game/ui/main_menu/main_menu_quit.png";
			image_h = "/game/ui/main_menu/main_menu_quit_s.png";
		},
		widget image 
		{
			scale = [ 0.25, 0.125, 1 ];
			trans = [ 0.85,0.135, 0 ];
