version = 1;
widget panel main_menu
{
	scale = [ 1, 1, 1 ];
	trans = [ 0, 0, 0 ];
	widget_list = [
		widget button startsp
		{
			scale = [ 0.4, 0.2, 1 ];
			trans = [ 0.5, 0.8,0 ];
			image = "/game/ui/main_menu/main_menu_sp.png";
			image_h = "/game/ui/main_menu/main_menu_sp_s.png";
		},
		widget button startmp
		{
			scale = [ 0.4, 0.2,  1 ];
